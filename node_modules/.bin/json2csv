#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/LibraryManagement/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/node_modules:/home/theia/LibraryManagement/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/node_modules:/home/theia/LibraryManagement/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules:/home/theia/LibraryManagement/node_modules/.pnpm/node_modules:/home/theia/LibraryManagement/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/LibraryManagement/node_modules/json2csv/bin/node_modules:/home/theia/LibraryManagement/node_modules/json2csv/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/LibraryManagement/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/node_modules:/home/theia/LibraryManagement/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/node_modules:/home/theia/LibraryManagement/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules:/home/theia/LibraryManagement/node_modules/.pnpm/node_modules:/home/theia/LibraryManagement/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/LibraryManagement/node_modules/json2csv/bin/node_modules:/home/theia/LibraryManagement/node_modules/json2csv/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json2csv/bin/json2csv.js" "$@"
else
  exec node  "$basedir/../json2csv/bin/json2csv.js" "$@"
fi
